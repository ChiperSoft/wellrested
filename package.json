{
  "name": "wellrested",
  "version": "1.1.0",
  "description": "A superagent wrapper library for creating preconfigured HTTP service clients",
  "main": "index.js",
  "scripts": {
    "test": "tap tests.js",
    "test:coverage": "tap --coverage tests.js",
    "test:travis": "npm run lint && npm run test:coverage",
    "lint": "eslint ./"
  },
  "engines": {
    "node": ">=4.2.0"
  },
  "keywords": [
    "http",
    "rest",
    "superagent",
    "promise",
    "client",
    "soa",
    "services"
  ],
  "author": "Jarvis Badgley <chiper@chipersoft.com> (http://chipersoft.com/)",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.4.6",
    "lodash.get": "~4.4.2",
    "lodash.set": "~4.3.2",
    "path-to-regexp": "^1.6.0",
    "superagent": "^3.1.0"
  },
  "devDependencies": {
    "eslint": "~3.14.1",
    "eslint-config-chiper": "*",
    "eslint-plugin-node": "~3.0.3",
    "eslint-plugin-promise": "~3.4.0",
    "nock": "~9.0.2",
    "tap": "~8.0.0"
  },
  "files": [
    "index.js"
  ],
  "nyc": {
    "exclude": [
      "tests.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ChiperSoft/wellrested.git"
  },
  "bugs": {
    "url": "https://github.com/ChiperSoft/wellrested/issues"
  }
}
