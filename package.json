{
  "name": "wellrested",
  "version": "1.0.0",
  "description": "A superagent wrapper library for creating preconfigured HTTP service clients",
  "main": "index.js",
  "scripts": {
    "test": "tap tests.js",
    "test:coverage": "tap --coverage tests.js",
    "lint": "eslint ./"
  },
  "engines": {
    "node": ">=4.2.0"
  },
  "keywords": [
    "http",
    "rest",
    "superagent",
    "promise",
    "client",
    "soa",
    "services"
  ],
  "author": "Jarvis Badgley <chiper@chipersoft.com> (http://chipersoft.com/)",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.4.6",
    "path-to-regexp": "^1.6.0",
    "superagent": "^2.3.0"
  },
  "devDependencies": {
    "eslint": "~3.8.1",
    "eslint-config-chiper": "*",
    "eslint-plugin-node": "~2.1.3",
    "eslint-plugin-promise": "~3.3.0",
    "nock": "~8.1.0",
    "tap": "~7.1.2"
  },
  "files": [
    "index.js"
  ],
  "nyc": {
    "exclude": [
      "tests.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ChiperSoft/wellrested.git"
  },
  "bugs": {
    "url": "https://github.com/ChiperSoft/wellrested/issues"
  }
}
