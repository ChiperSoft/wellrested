{
  "name": "wellrested",
  "version": "2.0.0",
  "description": "A superagent wrapper library for creating preconfigured HTTP service clients",
  "main": "index.js",
  "scripts": {
    "test": "tap tests.js",
    "test:coverage": "tap --coverage tests.js",
    "test:travis": "npm run lint && npm run test:coverage",
    "lint": "eslint --fix '**/*.js'"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "keywords": [
    "http",
    "rest",
    "superagent",
    "promise",
    "client",
    "soa",
    "services"
  ],
  "author": "Jocelyn Badgley <joc@twipped.com>",
  "license": "MIT",
  "dependencies": {
    "lodash.get": "~4.4.2",
    "lodash.set": "~4.3.2",
    "path-to-regexp": "~3.1.0",
    "superagent": "~5.1.0"
  },
  "devDependencies": {
    "eslint": "~6.3.0",
    "eslint-config-twipped": "~3.3.0",
    "eslint-plugin-node": "~10.0.0",
    "eslint-plugin-promise": "~4.2.1",
    "nock": "~11.3.3",
    "tap": "~14.6.1"
  },
  "files": [
    "index.js"
  ],
  "nyc": {
    "exclude": [
      "tests.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Twipped/wellrested.git"
  },
  "bugs": {
    "url": "https://github.com/Twipped/wellrested/issues"
  }
}
