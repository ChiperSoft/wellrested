{
  "name": "wellrested",
  "version": "1.1.0",
  "description": "A superagent wrapper library for creating preconfigured HTTP service clients",
  "main": "index.js",
  "scripts": {
    "test": "tap tests.js",
    "test:coverage": "tap --coverage tests.js",
    "test:travis": "npm run lint && npm run test:coverage",
    "lint": "eslint ./"
  },
  "engines": {
    "node": ">=4.2.0"
  },
  "keywords": [
    "http",
    "rest",
    "superagent",
    "promise",
    "client",
    "soa",
    "services"
  ],
  "author": "Jocelyn Badgley <joc@twipped.com>",
  "license": "MIT",
  "dependencies": {
    "bluebird": "~3.5.1",
    "lodash.get": "~4.4.2",
    "lodash.set": "~4.3.2",
    "path-to-regexp": "~2.1.0",
    "superagent": "^3.1.0"
  },
  "devDependencies": {
    "eslint": "~4.10.0",
    "eslint-config-chiper": "~2.2.0",
    "eslint-plugin-node": "~5.2.1",
    "eslint-plugin-promise": "~3.6.0",
    "nock": "~9.1.0",
    "tap": "~10.7.3"
  },
  "files": [
    "index.js"
  ],
  "nyc": {
    "exclude": [
      "tests.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Twipped/wellrested.git"
  },
  "bugs": {
    "url": "https://github.com/Twipped/wellrested/issues"
  }
}
